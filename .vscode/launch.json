{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Chrome",
        "request": "launch",
        "type": "chrome",
        "webRoot": "${workspaceFolder}"
      },
      {
        "name": "Attach to Edge",
        "port": 9222,
        "request": "attach",
        "type": "msedge",
        "webRoot": "${workspaceFolder}"
      },
        {
            "name": "Launch Edge",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
      {
        "name": "fukkChrome",
        "type": "chrome",
        "request": "launch",
        "file": "${file}",
        "webRoot": "${workspaceFolder}",
      }
    ]
  }
  